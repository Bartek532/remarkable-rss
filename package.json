{
  "name": "remarkable-rss",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "dev": "npm run docker:start:dev && next dev",
    "postinstall": "prisma generate",
    "prebuild": "rm -rf .next && prisma generate",
    "lint": "next lint",
    "lint:interactive": "eslint-interactive \"{src,apps,libs,test}/**/*.{ts,tsx}\"",
    "start": "next start",
    "db:migrate:dev": "dotenv -e .env.development.local -- prisma migrate dev",
    "docker:start:dev": "docker-compose -f provision/dev/docker-compose.yml up -d",
    "docker:stop:dev": "docker-compose -f provision/dev/docker-compose.yml down",
    "docker:build:dev": "docker-compose -f provision/dev/docker-compose.yml build",
    "docker:logs": "docker logs --follow rssmarkable"
  },
  "dependencies": {
    "@heroicons/react": "^2.0.13",
    "@hookform/resolvers": "^2.9.10",
    "@next-auth/prisma-adapter": "^1.0.5",
    "@prisma/client": "^4.5.0",
    "@tanstack/react-query": "^4.16.0",
    "@trpc/client": "^10.0.0",
    "@trpc/next": "^10.0.0",
    "@trpc/react-query": "^10.0.0",
    "@trpc/server": "^10.0.0",
    "bcrypt": "^5.1.0",
    "dayjs": "^1.11.7",
    "next": "13.1.1",
    "next-auth": "^4.18.3",
    "puppeteer-core": "^19.4.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.41.3",
    "react-hot-toast": "^2.4.0",
    "rmapi-js": "^2.0.0",
    "rss-parser": "^3.12.0",
    "superjson": "1.9.1",
    "v8": "^0.1.0",
    "zod": "^3.18.0"
  },
  "devDependencies": {
    "@svgr/webpack": "^6.5.1",
    "@tailwindcss/forms": "^0.5.3",
    "@types/bcrypt": "^5.0.0",
    "@types/node": "^18.0.0",
    "@types/prettier": "^2.7.2",
    "@types/react": "^18.0.14",
    "@types/react-dom": "^18.0.5",
    "@typescript-eslint/eslint-plugin": "^5.33.0",
    "@typescript-eslint/parser": "^5.33.0",
    "autoprefixer": "^10.4.7",
    "eslint": "^8.26.0",
    "eslint-config-next": "13.1.1",
    "eslint-interactive": "^10.3.0",
    "husky": "^8.0.2",
    "lint-staged": "^13.1.0",
    "postcss": "^8.4.14",
    "prettier": "^2.8.1",
    "prettier-plugin-tailwindcss": "^0.2.1",
    "prisma": "^4.5.0",
    "tailwindcss": "^3.2.0",
    "typescript": "^4.8.4"
  },
  "ct3aMetadata": {
    "initVersion": "6.11.5"
  },
  "lint-staged": {
    "src/**/*.{js,ts,jsx,tsx}": [
      "eslint --fix",
      "prettier --write \"**/*.{ts,tsx,md}\""
    ],
    "*.json": [
      "prettier --write"
    ]
  }
}
